<p id="notice"><%= notice %></p>

<h1>Listing Nsoj Tvs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Link</th>
      <th>Nsoj tv category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nsoj_tvs.each do |nsoj_tv| %>
      <tr>
        <td><%= nsoj_tv.title %></td>
        <td><%= nsoj_tv.link %></td>
        <td><%= nsoj_tv.nsoj_tv_category %></td>
        <td><%= link_to 'Show', nsoj_tv %></td>
        <td><%= link_to 'Edit', edit_nsoj_tv_path(nsoj_tv) %></td>
        <td><%= link_to 'Destroy', nsoj_tv, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nsoj tv', new_nsoj_tv_path %>
